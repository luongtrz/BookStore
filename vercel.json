{
    "version": 2,
    "builds": [
      {
        "src": "view/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "theme"
        }
      },
      {
        "src": "app/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app/$1"
      },
      {
        "src": "/(.*)",
        "dest": "view/$1"
      }
    ]
  }
  